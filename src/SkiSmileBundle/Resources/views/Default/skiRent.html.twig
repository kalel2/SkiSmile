<div class="ski-rent">
    <h4 class="block-header">{{ 'Ціни на прокат інветаря 2016-2017'|trans }}</h4>

    <table class="striped ski-table">
        <thead>
        <tr class="table-info">
            <th colspan="3"></th>
            <th>{{ '-5-7%' }}</th>
            <th>{{ '-7-10%' }}</th>
            <th>{{ '-10-15%' }}</th>
            <th></th>
        </tr>
        <tr class="table-header">
            <th>{{ 'Категорія'|trans }}</th>
            <th>{{ 'Інвентар'|trans }}</th>
            <th>{{ '1 день'|trans }}</th>
            <th>{{ '3-4 дні'|trans }}</th>
            <th>{{ '5-6 днів'|trans }}</th>
            <th>{{ '7-8 днів'|trans }}</th>
            <th>{{ 'Застава'|trans }}</th>
        </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td>{{ entity.category }}</td>
                <td>{{ entity.inventory }}</td>
                <td>{{ entity.priceOne }}</td>
                <td>{{ entity.priceTwo }}</td>
                <td>{{ entity.priceThree }}</td>
                <td>{{ entity.priceFour }}</td>
                <td>{{ entity.guarantee }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>